char *mystrcat(char *dst, char *src)<br />{<br /> char *p = dst;<br /> while(*p)++p;<br /> while(*p++ = *src++);<br /> return dst;<br />}<br />char *mystrcpy(char *dst, char *src)<br />{<br /> while(*dst++ = *src++);<br /> return dst;<br />}<br />int mystrcmp(char *dst, char *src)<br />{<br /> while(*dst && *dst == *src)<br /> {<br />  ++dst;<br />  ++src;<br /> }<br /> return *dst - *src;<br />}<br />int mystrlen(char *dst)<br />{<br /> char *p = dst;<br /> while(*p)++p;<br /> return p - dst;<br />}<br />int main()<br />{<br /> char s[32], t[32] = &quot;HELLO,CHINA&quot;;<br /> mystrcpy(s, &quot;HELLO,&quot;);<br /> puts(s);<br /> mystrcat(s, &quot;USA&quot;);<br /> puts(s);<br /> <br /> printf(&quot;%d %d&#92;n&quot;, mystrlen(s), mystrcmp(s, t));<br />}
