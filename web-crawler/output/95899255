int notPrime[101];<br /><br />int main()<br />{<br />    int i,j;<br />    for(i=2;i&lt;=100;i++)<br />        if(!notPrime[i]) {<br />            printf(&quot;%d is a prime number&#92;n&quot;,i);<br />            for(j=i*i;j&lt;=100;j+=i) notPrime[j] = 1;<br />        }<br />}
