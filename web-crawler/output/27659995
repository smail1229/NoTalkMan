#include&quot;stdlib.h&quot;<br />#include&quot;stdio.h&quot;<br />#define Maxsize 100<br />typedef char DataType;<br />typedef struct<br />{<br />DataType stack[Maxsize];<br />int top;<br />}SeqStack;<br />void StackInitiate(SeqStack *S)<br />{<br />S-&gt;top=0;<br />}<br />int StackNotEmpty(S俯缉碘垦鄢旧碉驯冬沫eqStack S)<br />{<br />if(S.top&lt;=0) return 0;<br />else return 1;<br />}<br />int StackPush(SeqStack *S,DataType x)<br />{<br />     if(S-&gt;top&gt;=Maxsize)<br />       {<br />        printf(&quot;The stack is full!&#92;n&quot;);<br />        return 0;<br />       }<br />    else<br />       {<br />        S-&gt;stack[S-&gt;top]=x;<br />        S-&gt;top++;<br />        return 1;<br />       }<br />}<br />int StackPop(SeqStack *S,DataType *d)<br />{<br />    if(S-&gt;top&lt;=0)<br />       {<br />        printf(&quot;The stack is empty&#92;n&quot;);<br />        return 0;<br />       }<br />    else<br />       {<br />        S-&gt;top--;<br />        *d=S-&gt;stack[S-&gt;top];<br />        return 1;<br />       }<br />}<br />int StackTop(SeqStack S,DataType *d)<br />{<br />   if(S.top&lt;=0)<br />      {<br />       printf(&quot;The stack is empty!&#92;n&quot;);<br />       return 0;<br />      }<br />   else<br />      {<br />       *d=S.stack[S.top-1];<br />       return 1;<br />      }<br />}<br /> 
#include&quot;stdlib.h&quot; <br />#include&quot;stdio.h&quot; <br />#define Maxsize 100 <br />typedef char DataType; <br />typedef struct 俯缉碘垦鄢旧碉驯冬沫  /*定义栈*/<br />{ <br />DataType stack[Maxsize]; <br />int top; <br />}SeqStack; <br />void StackInitiate(SeqStack *S)/*初始化栈，指针指向0*/<br />{ <br />S-&gt;top=0; <br />} <br />int StackNotEmpty(SeqStack S)/*判栈空，若空，返回0*/<br />{ <br />if(S.top&lt;=0) return 0; <br />else return 1; <br />} <br />int StackPush(SeqStack *S,DataType x) /*X进栈*/<br />{ <br />if(S-&gt;top&gt;=Maxsize)/*栈满*/<br />{ <br />printf(&quot;The stack is full!&#92;n&quot;); <br />return 0; <br />} <br />else <br />{ <br />S-&gt;stack[S-&gt;top]=x; <br />S-&gt;top++; <br />return 1; <br />} <br />} <br />int StackPop(SeqStack *S,DataType *d)  /*出栈，出栈的值传给*d*/<br />{ <br />if(S-&gt;top&lt;=0) /*栈空*/<br />{ <br />printf(&quot;The stack is empty&#92;n&quot;); <br />return 0; <br />} <br />else <br />{ <br />S-&gt;top--; <br />*d=S-&gt;stack[S-&gt;top]; <br />return 1; <br />} <br />} <br />int StackTop(SeqStack S,DataType *d) /*取栈顶*/<br />{ <br />if(S.top&lt;=0) <br />{ <br />printf(&quot;The stack is empty!&#92;n&quot;); <br />return 0; <br />} <br />else <br />{ <br />*d=S.stack[S.top-1]; <br />return 1; <br />} <br />}
