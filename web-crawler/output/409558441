#include &lt;stdio.h&gt;<br /><br />void fun(int *num)<br />{<br />	int temp;<br />	for(int i=0; i&lt;10; ++i)<br />		for(int j=0; j&lt;10-i-1; ++j)<br />		{<br />			if(num[j]&lt;num[j+1])<br />			{<br />				temp=num[j];<br />				num[j]=num[j+1];<br />				num[j+1]=temp;<br />			}<br />		}<br />}<br /><br />void main()<br />{<br />	int num[10]={3,2,4,1,5,6,4,3,9,8};<br />	fun(num);<br />	for(int i=0; i&lt;10; ++i)<br />		printf(&quot;%-2d&quot;,num[i]);<br />}
